db.product.find({cageory_id:{$ne:3}}, {product_id:1,product_name:1,_id:1}) // 后面的{}表示要显示的字段（1）， 0表示不需要显示，不写默认0不显示
db.product.find({product_price:{$gt:300}}) // >300
db.product.find({product_price:{$lte:100}}) // <= 100
db.product.find({cageory_id:{$in:[4,11]}})  // 分类为4或者11的所有商品
db.product.find({$and:[{product_price:{$gte:100}},{product_price:{$lte:500}}]}) // 100<=price<=500

// 分类不为3，11的所有商品, and  nin  nor实现
db.product.find({$and:[{cageory_id:{$ne:3}},{cageory_id:{$ne:11}}]}) 
db.product.find({cageory_id:{$nin:[3,11]}})
db.product.find({$nor:[{cageory_id:3},{cageory_id:11}]}) 
